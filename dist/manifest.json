{
    "manifest_version": 2,
    "name": "chatwork extension",
    "description": "a chatwork extension for google chrome",
    "version": "0.0.1",
    "icons": {
        "16": "icon_16.png",
        "48": "icon_48.png",
        "128": "icon_128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.chatwork.com/*"
            ],
            "js": [
                "index.js"
            ],
            "css": [
                "index.css"
            ]
        }
    ]
}
